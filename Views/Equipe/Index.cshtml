@model Equipe
@{
    ViewData["title"] = "Equipes";
}

<form method="POST" action='@Url.Action("Cadastrar")'>
    <label>ID</label>
    <input type="text" name="Id_equipe">
    
    <label>Nome</label>
    <input type="text" name="Nome">
    
    <label>Imagem</label>
    <input type="text" name="Imagem">

    <button type="submit">Cadastrar</button>
</form>

<table class="table table-striped table-responsive">
    <thead>
        <th>Id</th>
        <th>Nome</th>
        <th>Foto</th>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.Equipes)
        {
            <tr>
                <td>@item.Id_equipe</td>
                <td>@item.Nome</td>
                <td>@item.Imagem</td>
            </tr>
        }
    </tbody>
</table>